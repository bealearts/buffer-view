{
  "name": "serial-data-view",
  "version": "0.0.0",
  "description": "Serial, Heterogeneous access to an ArrayBuffer",
  "type": "module",
  "main": "index.mjs",
  "types": "src/index.ts",
  "scripts": {
    "test": "asp --verbose",
    "build:untouched": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts src/index.ts -b dist/untouched.wasm -t dist/untouched.wat --sourceMap --debug",
    "build:optimized": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts src/index.ts -b dist/optimized.wasm -t dist/optimized.wat --sourceMap --optimize",
    "build": "npm run build:untouched && npm run build:optimized"
  },
  "keywords": [
    "data",
    "view",
    "buffer",
    "binary",
    "assemblyscript"
  ],
  "author": "David Beale",
  "license": "MIT",
  "devDependencies": {
    "@as-pect/cli": "^3.2.3",
    "assemblyscript": "^0.10.0"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.10.0",
    "as-bind": "^0.3.4"
  }
}
